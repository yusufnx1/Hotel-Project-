@model List<ResultAboutDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık 1</th>
        <th>Başlık 2</th>
        <th>Güncelle</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>@item.Title1</td>
            <td>@item.Title2</td>
            <td><a href="/AdminAbout/UpdateAbout/@item.AboutID" class="btn btn-outline-warning">Güncelle</a></td>
        </tr>
    }
</table>