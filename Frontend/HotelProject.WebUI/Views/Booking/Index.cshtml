
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
   @await Component.InvokeAsync("_HeadPartial")
</head>

<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
       @await Component.InvokeAsync("_SpinnerPartial")
        <!-- Spinner End -->
        <!-- Header Start -->
        @await Component.InvokeAsync("_NavHeader")

        @await Component.InvokeAsync("_BookingCoverPartial")

        <!-- Header End -->
        @await Html.PartialAsync("~/Views/Booking/BookingAdd.cshtml")
        <!-- Newsletter Start -->
        @await Html.PartialAsync("~/Views/Defalt/_SubscribePartial.cshtml")
        <!-- Newsletter Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("_FooterPartial")
        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    @await Component.InvokeAsync("_ScriptPartial")
</body>

</html>